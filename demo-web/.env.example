# Rankify Demo Environment Variables - FILL IN YOUR VALUES
# Copy this to .env.local (never commit .env.local to git)

# Azure OpenAI API (for generation)
RANKIFY_AZURE_ENDPOINT=https://YOUR_RESOURCE.openai.azure.com/
RANKIFY_AZURE_KEY=YOUR_KEY_HERE
RANKIFY_AZURE_DEPLOYMENT=gpt-4o
RANKIFY_AZURE_API_VERSION=2025-01-01-preview

# Python demo server URL (defaults to localhost:8000)
RANKIFY_API_URL=http://localhost:8000
